{
    "executionRoleArn": "arn:aws:iam::959764943449:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "my-new-revision",
            "image": "959764943449.dkr.ecr.us-east-1.amazonaws.com/my-hello-image",
            "memory": 200,
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 90,
                    "containerPort": 90,
                    "protocol": "tcp"
                }
            ]
        },
	{
            "memory": 400,
            "image": "959764943449.dkr.ecr.us-east-1.amazonaws.com/my-hello-image",
            "essential": true,
            "name": "my-final",
            "portMappings": [
                {
                    "hostPort": 60,
                    "protocol": "tcp",
                    "containerPort": 60
                }
            ]
        }
    ],
    "memory": "2048",
    "taskRoleArn": "arn:aws:iam::959764943449:role/ecsTaskExecutionRole",
    "family": "new-task-def",
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "bridge",
    "cpu": "1024"
}
